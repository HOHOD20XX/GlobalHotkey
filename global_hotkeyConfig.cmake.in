@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Threads REQUIRED)
if(NOT @GBHK_DISABLE_HOOK@)
    find_dependency(kbdt REQUIRED)
endif()
if(APPLE)
    find_dependency(Carbon CoreFoundation REQUIRED)
endif()
if(NOT @GBHK_DISABLE_REGISTER@ AND @LINUX@)
    find_dependency(X11)
endif()

include(@PACKAGE_LIB_DIR@/cmake/@PROJECT_NAME@Targets.cmake)

set_and_check(@PROJECT_NAME@_INCLUDE_DIRS @PACKAGE_INCLUDE_DIRS@)
set_and_check(@PROJECT_NAME@_LIBRARY_DIRS @PACKAGE_LIB_DIR@)
set(@PROJECT_NAME@_LIBRARIES @PROJECT_NAME@::@PROJECT_NAME@)

set(GBHK_DISABLE_HOOK @GBHK_DISABLE_HOOK@)
set(GBHK_DISABLE_REGISTER @GBHK_DISABLE_REGISTER@)
set(GBHK_BUILD_SHARED @GBHK_BUILD_SHARED@)
set(@PROJECT_NAME@_FOUND TRUE)
check_required_components(@PROJECT_NAME@)
